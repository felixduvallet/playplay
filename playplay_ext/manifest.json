{
  "name": "PlayPlay",
  "description": "Global shortcut keys for Google Play Music",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://play.google.com/music/*"],
      "js": ["playplay_client.js" ]
    }
  ],

  "permissions": [
    "https://play.google.com/"
  ],

  "manifest_version": 2
}
